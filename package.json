    "redux-mock-store": "1.2.3",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.5",
    "style-loader": "0.17.0",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0"
  },
  "browserslist": [
    "last 2 versions",
    ">1%",
    "not ie < 11"
  ],
  "repository": {
